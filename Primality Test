class Solution {
    public boolean isPrime(int N) {
      // code here
      boolean x=true;
    for ( int i=2;i<=Math.sqrt(N);i++)
    {
        if (N%i==0)
        {
            x=false;
            break;
        }
    }
    
    return x;
    }
}
